{
  "name": "Light/Light",
  "type": "project",
  "description": "Light Framework.",
  "keywords": ["Light", ,"framework"],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-mysqli": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "Core/"
    }
